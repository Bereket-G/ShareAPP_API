{
  "name": "post",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "file": {
      "type": "string"
    },
    "createdAt": {
      "type": "date",
      "default": "$now"
    },
    "votes": {
      "type": [
        "string"
      ]
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": ""
    },
    "comments": {
      "type": "hasMany",
      "model": "comment",
      "foreignKey": ""
    },
    "topics": {
      "type": "hasMany",
      "model": "topic",
      "foreignKey": "postId",
      "through": "post_topic"
    }
  },
  "acls": [],
  "methods": {}
}
